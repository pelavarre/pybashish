    $

    $ (cd lsdir/ && ../bin/ls.py -1)
    dir
    execable
    file
    link
    $

    $ (cd lsdir/ && ../bin/ls.py -al)
    drwxr-xr-x  .  .  .  .  ...  .
    drwxr-xr-x  .  .  .  .  ...  ..
    drwxr-xr-x  .  .  .  .  ...  dir
    -rwxr-xr-x  .  .  .  0  ...  execable
    -rw-r--r--  .  .  .  0  ...  file
    -rw-r--r--  .  .  .  0  ...  link
    $

    $ (cd lsdir/ && ../bin/ls.py -l)
    drwxr-xr-x  .  .  .  .  ...  dir
    -rwxr-xr-x  .  .  .  0  ...  execable
    -rw-r--r--  .  .  .  0  ...  file
    -rw-r--r--  .  .  .  0  ...  link
    $

    $ (cd lsdir/ && ../bin/ls.py -CFr)
